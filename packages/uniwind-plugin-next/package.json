{
  "name": "uniwind-plugin-next",
  "version": "1.4.0",
  "author": "Alexander Nicholson",
  "description": "Compatibility plugin for using Uniwind with Next.js",
  "homepage": "https://github.com/a16n-dev/uniwind-plugin-next#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/a16n-dev/uniwind-plugin-next.git"
  },
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "devDependencies": {
    "@vitest/ui": "^4.0.16",
    "postcss": "^8.5.6",
    "tsdown": "^0.18.2",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@tailwindcss/node": "^4",
    "lightningcss": ">=1.30.2",
    "postcss": "^8",
    "uniwind": "^1.2.7",
    "webpack": "^5"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./postcss": {
      "require": "./dist/postcss/index.cjs",
      "import": "./dist/postcss/index.mjs"
    },
    "./package.json": "./package.json",
    "./css": {
      "style": "./dist/webpack/uniwind/uniwind.css"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist",
    "README.md"
  ]
}
